{
  "name": "ralph-dev",
  "version": "0.4.8",
  "description": "Autonomous end-to-end development system - from requirement to production-ready code with zero manual intervention",
  "author": {
    "name": "Lukin",
    "email": "[email protected]"
  },
  "homepage": "https://github.com/mylukin/ralph-dev",
  "repository": "https://github.com/mylukin/ralph-dev",
  "license": "MIT",
  "keywords": [
    "ralph-dev",
    "autonomous",
    "ai-agent",
    "code-generation",
    "tdd",
    "multi-language",
    "self-healing",
    "task-management",
    "workflow",
    "pr-automation"
  ],
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/.claude-plugin/hooks/session-start.sh"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/.claude-plugin/hooks/pre-compact.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/.claude-plugin/hooks/stop-hook.sh"
          }
        ]
      }
    ]
  }
}
